{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(levelname)s - %(filename)s - %(funcName)s -%(process)d - %(message)s"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        },
        "FileHandler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "INFO",
            "formatter": "simple",
            "filename": "Logs/logDataMemoTransfer.log",
            "maxBytes": 10485760,
            "backupCount": 10,
            "encoding": "utf-8"
        }
    },
    "loggers": {
        "__main__": {
            "level": "DEBUG",
            "handlers": [
                "consoleHandler",
                "FileHandler"
            ],
            "propagate": false
        },
        "data_memo_transfer": {
            "level": "DEBUG",
            "handlers": [
                "consoleHandler",
                "FileHandler"
            ],
            "propagate": false
        }
    },
    "root": {
        "level": "INFO"
    }
}
