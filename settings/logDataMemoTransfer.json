{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "display": {
            "format": "%(asctime)s - %(levelname)s - %(funcName)s - %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        },
        "display_error": {
            "format": "%(levelname)s, \"%(pathname)s\" line %(lineno)d in %(module)s.%(funcName)s\n----------------------------------------",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        },
        "file": {
            "format": "%(asctime)s, %(levelname)s, %(process)d/%(threadName)s:%(thread)d, %(filename)s:%(lineno)d, %(module)s.%(funcName)s, %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "display",
            "stream": "ext://sys.stdout"
        },
        "consoleHandlerError": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "display_error",
            "stream": "ext://sys.stdout"
        },
        "FileHandler": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "INFO",
            "formatter": "file",
            "filename": "Logs/logDataMemoTransfer.log",
            "maxBytes": 10485760,
            "backupCount": 10,
            "encoding": "utf-8"
        }
    },
    "loggers": {
        "__main__": {
            "level": "DEBUG",
            "handlers": [
                "consoleHandler",
                "consoleHandlerError",
                "FileHandler"
            ],
            "propagate": false
        },
        "data_memo_transfer": {
            "level": "INFO",
            "handlers": [
                "FileHandler"
            ],
            "propagate": false
        },
        "data_memo_transfer_debug": {
            "level": "DEBUG",
            "handlers": [
                "consoleHandler",
                "consoleHandlerError",
                "FileHandler"
            ],
            "propagate": false
        }
    },
    "root": {
        "level": "INFO",
        "handlers": [
            "consoleHandler"
        ]
    }
}
